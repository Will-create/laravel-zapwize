{
    "name": "zapwize/laravel",
    "description": "Laravel package for Zapwize WhatsApp Web API integration",
    "homepage": "https://github.com/Will-create/laravel-zapwize",
    "support": {
        "issues": "https://github.com/Will-create/laravel-zapwize/issues",
        "source": "https://github.com/Will-create/laravel-zapwize"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/Will-create"
        }
    ],
    "keywords": [
        "laravel",
        "whatsapp",
        "api",
        "zapwize",
        "messaging",
        "webhook"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zapwize Team",
            "email": "support@zapwize.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/events": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/queue": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "ratchet/pawl": "^0.4.3"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.4",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Zapwize\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zapwize\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zapwize\\Laravel\\ZapwizeServiceProvider"
            ],
            "aliases": {
                "Zapwize": "Zapwize\\Laravel\\Facades\\Zapwize"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint"
    }
}
